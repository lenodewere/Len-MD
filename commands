  .react-code-view-header-wrap--narrow { flex-wrap: wrap; }
}

@container (min-width: 768px) {
  .react-code-view-header-element--wide { display: flex !important; }
  .react-code-view-header-element--narrow { display: none !important; }
}

@supports not (container-type: inline-size) {
  @media (max-width: 768px) {
  .react-code-view-header-element--wide { display: none !important; }
  .react-code-view-header-element--narrow { width: 100%; display: flex !important; }
}
  @media (min-width: 768px) {
  .react-code-view-header-element--wide { width: 100%; display: flex !important; }
  .react-code-view-header-element--narrow { display: none !important; }
}
}

